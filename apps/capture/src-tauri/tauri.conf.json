{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "SerialWarp Capture",
  "version": "0.0.1-alpha.1",
  "identifier": "com.serialwarp.capture",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "SerialWarp Capture",
        "width": 800,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 700,
        "minHeight": 550
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJDQ0QxQzY3QzBDOUFGRDMKUldUVHI4bkFaeHpOTENhK3JaL09GMGVwOEtERXFwUnl4cTVMREx4OGpJVndvSHF1OVYvRGwzcTcK",
      "endpoints": [
        "https://github.com/Achxy/serialwrap/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns"
    ],
    "createUpdaterArtifacts": true,
    "macOS": {
      "minimumSystemVersion": "12.3",
      "entitlements": "Entitlements.plist",
      "frameworks": [],
      "exceptionDomain": null
    }
  }
}
